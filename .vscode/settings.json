{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.next": true,
        ".next": true,
        "**/node_modules": true,
        "public": true
    },
    "explorerExclude.backup": {},
    "favorites.resources": [
        {
            "filePath": "public",
            "group": "Default"
        }
    ],
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
      },
    "editor.formatOnSaveMode": "modificationsIfAvailable",
}